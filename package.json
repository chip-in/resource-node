{
  "name": "@chip-in/resource-node",
  "version": "0.0.9",
  "description": "chip-in ResourceNode",
  "keywords": [
    "chip-in"
  ],
  "bugs": {
    "url": "https://github.com/chip-in/resource-node/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "jsdoc": "jsdoc --configure jsdoc.json --template node_modules/jsdoc-baseline/ -d api -r",
    "cleanbuild": "npm run clean && npm run build",
    "build": "babel src -s inline -d lib/",
    "clean": "del-cli lib",
    "watch": "npm run clean && babel src -s inline -d lib/ -w",
    "prepare": "npm run cleanbuild"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cpx": "^1.5.0",
    "del-cli": "^1.1.0",
    "esdoc": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "jsdoc": "^3.5.5",
    "jsdoc-baseline": "https://github.com/hegemonic/baseline/tarball/master"
  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "into-stream": "^3.1.0",
    "mongo-parse": "^2.0.3",
    "morgan": "^1.9.0",
    "mqtt": "^2.13.0",
    "node-fetch": "^1.7.3",
    "nunjucks": "^3.0.1",
    "socket.io-client": "^2.0.3",
    "uuid": "^3.1.0",
    "yamljs": "^0.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
