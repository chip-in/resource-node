{
  "name": "@chip-in/resource-node",
  "version": "0.0.3",
  "description": "chip-in ResourceNode",
  "keywords": [
    "chip-in"
  ],
  "bugs": {
    "url": "https://github.com/chip-in/resource-node/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "esdoc": "esdoc -c ./esdoc.json",
    "cleanbuild": "npm run clean && npm run build",
    "build": "babel src -s inline -d lib/",
    "clean": "del-cli lib",
    "watch": "npm run clean && babel src -s inline -d lib/ -w",
    "example-cleanbuild": "npm run example-clean && npm run example-build",
    "example-build": "babel examples/src -s inline -d examples/lib/ --copy-files",
    "example-clean": "del-cli examples/lib",
    "example-watch": "npm run example-clean && babel examples/src -s inline -d examples/lib/ --copy-files -w",
    "prepare": "npm run cleanbuild"
  },
  "devDependencies": {
    "esdoc": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cpx": "^1.5.0",
    "del-cli": "^1.1.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "mongo-parse": "^2.0.3",
    "morgan": "^1.9.0",
    "mqtt": "^2.13.0",
    "node-fetch": "^1.7.3",
    "socket.io-client": "^2.0.3",
    "uuid": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
